<h1>Listing scans</h1>

<table>
  <tr>
    <th>Total user</th>
    <th>Total stock truck</th>
    <th>Total stock cargo</th>
    <th>Idle stock truck</th>
    <th>Idle stock cargo</th>
    <th>Total cargo</th>
    <th>Total truck</th>
    <th>expired cargo</th>
    <th>Expired truck</th>
    <th>Total line</th>
    <th>Total company</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @scans.each do |scan| %>
  <tr>
    <td><%= scan.total_user %></td>
    <td><%= scan.total_stock_truck %></td>
    <td><%= scan.total_stock_cargo %></td>
    <td><%= scan.idle_stock_truck %></td>
    <td><%= scan.idle_stock_cargo %></td>
    <td><%= scan.total_cargo %></td>
    <td><%= scan.total_truck %></td>
    <td><%= scan.expired_cargo %></td>
    <td><%= scan.expired_truck %></td>
    <td><%= scan.total_line %></td>
    <td><%= scan.total_company %></td>
    <td><%= scan.user_id %></td>
    <td><%= link_to 'Show', scan %></td>
    <td><%= link_to 'Edit', edit_scan_path(scan) %></td>
    <td><%= link_to 'Destroy', scan, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<%= will_paginate @scans ,:class=>"page_navi_link"%>

<br />

<%= link_to 'New Scan', new_scan_path %>
